{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-class-properties",
    "transform-object-assign",
    // Allows us to use ESModule includes from src instead of using relative paths everywhere
    ["module-resolver", {
      "root": ["./src"]
    }]
  ]
}
